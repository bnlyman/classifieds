<div class = "jumbotron">
	<h1> Post new item </h1>
	<%= form_for @item do |f| %>
	  <%= f.label :category %>: 
	  <%= f.text_field :category %>

	  <%= f.label :name %> 
	  <%= f.text_field :name %>

	  <%= f.label :price %>
	  <%= f.text_field :price %>

	  <%= f.label :description %>
	  <%= f.text_field :description %>

	  <%= f.submit %>
	<% end %>
</div>